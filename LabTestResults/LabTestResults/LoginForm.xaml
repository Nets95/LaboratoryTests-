<Window x:Class="LabTestResults.LoginForm"
        
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Name ="loginW" Title="LoginForm" Height="175" Width="292.373">
    <Grid Margin="0,0,3,0">
        <Button Content="Log on" HorizontalAlignment="Left" Margin="106,106,0,0" VerticalAlignment="Top" Width="75" Click="logIn"/>
        <TextBox x:Name="txtLogin" HorizontalAlignment="Left" Height="23" Margin="133,27,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="txtPassword" HorizontalAlignment="Left" Height="23" Margin="134,65,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Login" HorizontalAlignment="Left" Margin="26,25,0,0" VerticalAlignment="Top" Width="100"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="28,62,0,0" VerticalAlignment="Top" Width="100"/>

    </Grid>
</Window>
